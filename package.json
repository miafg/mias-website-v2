{
  "name": "mias-website",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "server.js",
  "dependencies": {
    "aws-sdk": "^2.421.0",
    "babel-cli": "^6.26.0",
    "cloudinary-react": "^1.1.0",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "nodemon": "^1.18.10",
    "request": "^2.88.0"
  },
  "devDependencies": {},
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run react-start\"",
